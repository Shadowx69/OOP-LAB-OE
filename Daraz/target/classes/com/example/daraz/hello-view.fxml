<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Label?>

<?import javafx.geometry.Insets?>
<TilePane fx:id="tilePane" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.daraz.HelloController">
    <!-- Example of a product VBox. This will be created dynamically in your Java code. -->
    <VBox spacing="5">
        <padding>
            <Insets top="10" right="10" bottom="10" left="10" />
        </padding>
        <ImageView fitHeight="150" fitWidth="150" />
        <Label text="Product Name" />
        <Label text="Product Price" />
    </VBox>
</TilePane>
